<!DOCTYPE html>
<html lang="en">

<head>
  <title>Contact Us</title>
  <%- include('./partials/head'); %>
</head>

<body class="page homepage">
  <input type="hidden" id="nav_id" value="contactUs">
  <header>
    <%- include('./partials/header'); %>
  </header>

  <!-- content -->
  <div class="content">
    <section class="contact-us-wrap ptb-100">
      <div class="container">
        <div class="row justify-content-center pb-75">
          <div class="col-xl-4 col-lg-6 col-md-6">
            <div class="contact-item">
              <span class="contact-icon">
                <i class="fa fa-map-marker"></i>
              </span>
              <div class="contact-info">
                <h3>Visit Us Anytime</h3>
                <p>xxxxxxx xxxxxxxx xxxxxxxx xxxxxxx xxxxxxxxx</p>
              </div>
            </div>
          </div>
          <div class="col-xl-4 col-lg-6 col-md-6">
            <div class="contact-item">
              <span class="contact-icon">
                <i class="fa fa-envelope"></i>
              </span>
              <div class="contact-info">
                <h3>Send An Email</h3>
                <a href="mailto:info@carelux.in">info@carelux.in</a>
              </div>
            </div>
          </div>
          <div class="col-xl-4 col-lg-6 col-md-6">
            <div class="contact-item">
              <span class="contact-icon">
                <i class="fa fa-phone"></i>
              </span>
              <div class="contact-info">
                <h3>Call Center</h3>
                <a href="tel:+91-89896 40406">+91-89896 40406</a>
              </div>
            </div>
          </div>
        </div>
        <div class="row gx-5">
          <div class="col-xl-8 col-lg-7 col-12">
            <div class="contact-form">
              <h3>Send Us A Message</h3>
              <form action="do-inquiry" method="POST" class="form-wrap" id="contactForm" novalidate="true">
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="text" placeholder="Name*" id="inquirerName" name="inquirerName" data-error="Please enter your name" />
                      <div class="help-block with-errors"></div>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group has-error">
                      <input type="email" name="inquirerEmail" id="inquirerEmail" placeholder="Email*" data-error="Please enter your email" />
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="number" name="inquirerPhone" id="inquirerPhone" placeholder="Phone Number*" data-error="Please enter your email" />
                      <div class="help-block with-errors"></div>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="text" placeholder="Subject*" id="inquirySubject" name="inquirySubject" data-error="Please enter your subject" />
                      <div class="help-block with-errors"></div>
                    </div>
                  </div>
                  <div class="col-md-12">
                    <div class="form-group v1">
                      <textarea name="inquiryMessage" id="inquiryMessage" placeholder="Your Messages.." cols="30" rows="10" required="" data-error="Please enter your message"></textarea>
                      <div class="help-block with-errors"></div>
                    </div>
                  </div>
                  <div class="col-md-12">
                    <button type="submit" class="btn style1" style="pointer-events: all; cursor: pointer">
                      Send Message
                    </button>
                    <div id="msgSubmit" class="h3 text-center hidden"></div>
                    <div class="clearfix"></div>
                  </div>
                </div>
              </form>
            </div>
          </div>
          <div class="col-xl-4 col-lg-5 col-md-12">
            <div class="comp-map">
              <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3151.8385385572983!2d144.95358331584498!3d-37.81725074201705!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6ad65d4dd5a05d97%3A0x3e64f855a564844d!2s121%20King%20St%2C%20Melbourne%20VIC%203000%2C%20Australia!5e0!3m2!1sen!2sbd!4v1612419490850!5m2!1sen!2sbd">
              </iframe>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
  <!-- ./ content -->

  <footer>
    <%- include('./partials/footer'); %>
  </footer>
  <script src="https://unpkg.com/just-validate@latest/dist/just-validate.production.min.js"></script>
  <script src="../js/validator/contactUs_form_validator.js"></script>
</body>

</html>